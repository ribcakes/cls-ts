{
  "name": "cls-ts",
  "version": "0.1.0",
  "description": "CLS implemented with TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "compile": "tsc",
    "debug": "node --nolazy --debug-brk=5858",
    "fix": "prettier --write \"{src,test}/**/*.ts\" && eslint --fix \"{src,test}/**/*.ts\"",
    "lintSrc": "eslint \"src/**/*.ts\"",
    "lintTest": "eslint --config .eslintrc.test.js \"test/**/*.ts\"",
    "lint": "npm run lintSrc && npm run lintTest",
    "test": "jest",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run compile && npm run typedocs",
    "typedocs": "typedoc --ignoreCompilerErrors --out ./docs ./src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ribcakes/cls-ts.git"
  },
  "keywords": [
    "threading",
    "shared",
    "context"
  ],
  "author": "Forrest L Norvell <ogd@aoaioxxysz.net>",
  "contributors": [
    "Tim Caswell <tim@creationix.com>",
    "Forrest L Norvell <ogd@aoaioxxysz.net>",
    "Jeff Lewis <jlewis@streetconnect.com>",
    "Brian <ribcakes@ribcakes.com>"
  ],
  "license": "BSD-2-Clause",
  "engineStrict": false,
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "emitter-listener": "^1.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.28",
    "@types/jest": "^24.0.22",
    "@types/node": "^10.17.4",
    "@types/superagent": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "bluebird": "^3.7.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ],
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "interface"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "lines": 100
      }
    }
  }
}
